<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <StartupObject>Contacts.WPF.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MySql.Data" Version="8.0.23" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.2" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="MyControls">
      <HintPath>..\..\MyControls\bin\Debug\netcoreapp3.1\MyControls.dll</HintPath>
    </Reference>
  </ItemGroup>

  <Import Project="..\Contacts\Contacts.projitems" Label="Shared" />

</Project>